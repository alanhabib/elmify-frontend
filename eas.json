{
  "cli": {
    "promptToConfigurePushNotifications": false,
    "version": ">= 16.19.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Debug",
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "simulator": false
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production"
      },
      "ios": {
        "buildConfiguration": "Release",
        "credentialsSource": "remote",
        "simulator": false,
        "image": "latest",
        "cache": {
          "disabled": false
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "1234567890",
        "appleTeamId": "RJ59PXFA29",
        "ascApiKeyPath": "./AuthKey.p8",
        "ascApiKeyIssuerId": "$APP_STORE_CONNECT_API_ISSUER_ID",
        "ascApiKeyId": "$APP_STORE_CONNECT_API_KEY_ID"
      }
    }
  }
}
